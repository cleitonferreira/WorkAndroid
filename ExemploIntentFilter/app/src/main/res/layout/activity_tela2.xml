        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voltar"
        android:id="@+id/button2"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:onClick="voltarTela2"/>
</RelativeLayout>
